{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true,
    "browser": true
  },
  "globals": {
    "google": true,
    "UI": true,
    "getEnv": true,
    "objectify": true,
    "getDashboard": true,
    "cloudinary": true,
    "requirejs": true,
    "Cleave": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-shadow": 0,
    "no-alert": 0,
    "import/no-cycle": 0,
    "prefer-rest-params": 0,
    "no-restricted-syntax": 0
  }
}
