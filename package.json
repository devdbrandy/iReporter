{
  "name": "ireporter",
  "version": "1.0.0",
  "description": "iReporter app enables users (citizen) to bring any form of corruption to the notice of appropriate authorities and the general public",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && babel src -d dist",
    "build:serve": "npm run build & npm run serve",
    "dev": "babel-node src/index.js -r dotenv/config",
    "clean": "rimraf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "gh:deploy": "gh-pages -d UI",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build ",
    "lint": "eslint src/",
    "mocha": "cross-env NODE_ENV=test mocha --require babel-register",
    "mocha:watch": "npm run mocha -- -w",
    "test": "nyc --reporter=html --reporter=text mocha --require babel-register",
    "serve": "npm run start -- -r dotenv/config",
    "start": "node dist/index.js",
    "watch": "nodemon --exec npm run dev"
  },
  "keywords": [
    "reports",
    "corruption",
    "intervention",
    "red flag",
    "citizens"
  ],
  "author": "Harry Dbrandy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.4.0",
    "loglevel": "^1.6.1",
    "mocker-data-generator": "^2.6.6",
    "morgan": "~1.9.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "editorconfig": "^0.15.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "gh-pages": "^2.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1",
    "supertest": "^3.3.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "engines": {
    "node": "10.x"
  }
}
